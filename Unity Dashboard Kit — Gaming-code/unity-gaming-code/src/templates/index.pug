extends layouts/_layout
block content
	.page
		+sidebar('active')
		.page__wrapper
			+header
			.main.js-main
				.main__container
					.main__for.js-slider.js-main-for
						mixin slideBig(image)
							.main__slide
								.main__item(style="background-image: url(" + image + ");")
									.main__details
										.main__live.live.live_big Live
										.main__title.h4 Renegades vs Chiefs - ESL Proâ€¨League Season 16 - Playoffs
										.main__parameters
											.main__parameter
